<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#ffffff">


    <FrameLayout
        android:id="@+id/mainmenu"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:id="@+id/mainframe"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff"
            android:visibility="visible">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="731dp"
                android:orientation="vertical">

                <View
                    android:id="@+id/view2"
                    android:layout_width="match_parent"
                    android:layout_height="12dp" />

                <ImageView
                    android:id="@+id/notice"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/group_7" />

                <View
                    android:id="@+id/view6"
                    android:layout_width="match_parent"
                    android:layout_height="10dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:orientation="horizontal">

                    <View
                        android:id="@+id/view5"
                        android:layout_width="30dp"
                        android:layout_height="45dp"
                        android:layout_weight="0" />

                    <TextView
                        android:id="@+id/da"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/inter_black"
                        android:letterSpacing="0.03"
                        android:text="DA"
                        android:textColor="#1B1B1B"
                        android:textSize="32dp" />

                    <TextView
                        android:id="@+id/p"
                        android:layout_width="52dp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/inter_black"
                        android:letterSpacing="0.03"
                        android:text=":P"
                        android:textColor="#024C6F"
                        android:textSize="32dp" />

                    <View
                        android:id="@+id/view3"
                        android:layout_width="52dp"
                        android:layout_height="45dp"
                        android:layout_weight="1" />

                    <ImageButton
                        android:id="@+id/imageView888"
                        android:layout_width="23dp"
                        android:layout_height="33dp"
                        android:layout_marginTop="12dp"
                        android:layout_weight="0"
                        android:backgroundTint="#00FFFFFF"
                        android:padding="0dp"
                        android:scaleType="fitCenter"
                        app:srcCompat="@drawable/group_8__2_" />

                    <View
                        android:id="@+id/view8"
                        android:layout_width="10dp"
                        android:layout_height="45dp"
                        android:layout_weight="0" />

                    <ImageButton
                        android:id="@+id/menu_btn"
                        android:layout_width="23dp"
                        android:layout_height="22dp"
                        android:layout_marginTop="17dp"
                        android:layout_weight="0"
                        android:backgroundTint="#00FFFFFF"
                        android:padding="0dp"
                        android:scaleType="fitCenter"
                        app:srcCompat="@drawable/group_9__1_" />

                    <View
                        android:id="@+id/view"
                        android:layout_width="30dp"
                        android:layout_height="45dp"
                        android:layout_weight="0" />
                </LinearLayout>

                <!-- 상단 아이콘 모음 영역 -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:layout_margin="8dp"
                    android:background="@drawable/rectangle"
                    android:elevation="8dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="0dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <!-- 정부24 -->
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageButton
                                android:id="@+id/gov"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:backgroundTint="#00FFFFFF"
                                android:padding="0dp"
                                android:scaleType="fitCenter"
                                app:srcCompat="@drawable/image_17" />

                            <TextView
                                android:id="@+id/govern_name"
                                android:layout_width="wrap_content"
                                android:layout_height="18dp"
                                android:text="정부24"
                                android:textAlignment="center"
                                android:textSize="12dp" />
                        </LinearLayout>

                        <!-- 워크넷 -->

                        <View
                            android:layout_width="18dp"
                            android:layout_height="wrap_content" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageButton
                                android:id="@+id/work24"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:backgroundTint="#00FFFFFF"
                                android:padding="0dp"
                                android:scaleType="fitCenter"
                                app:srcCompat="@drawable/image_17" />

                            <TextView
                                android:id="@+id/worknet_name"
                                android:layout_width="wrap_content"
                                android:layout_height="18dp"
                                android:text="고용24"
                                android:textAlignment="center"
                                android:textSize="12dp" />
                        </LinearLayout>

                        <!-- 캘린더 -->
                        <View
                            android:id="@+id/view11"
                            android:layout_width="18dp"
                            android:layout_height="wrap_content" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageButton
                                android:id="@+id/calendar_image"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:backgroundTint="#00FFFFFF"
                                android:padding="5dp"
                                android:scaleType="fitCenter"
                                app:srcCompat="@drawable/date" />

                            <TextView
                                android:id="@+id/calendar_name"
                                android:layout_width="wrap_content"
                                android:layout_height="18dp"
                                android:text="달력"
                                android:textAlignment="center"
                                android:textSize="12dp" />
                        </LinearLayout>

                        <!-- 사전 -->
                        <View
                            android:id="@+id/view12"
                            android:layout_width="18dp"
                            android:layout_height="wrap_content" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageButton
                                android:id="@+id/dic_image"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:backgroundTint="#00FFFFFF"
                                android:padding="5dp"
                                android:scaleType="fitCenter"
                                app:srcCompat="@drawable/dic" />

                            <TextView
                                android:id="@+id/dic_name"
                                android:layout_width="wrap_content"
                                android:layout_height="18dp"
                                android:text="사전"
                                android:textAlignment="center"
                                android:textSize="12dp" />
                        </LinearLayout>

                        <!-- 질문 -->
                        <View
                            android:id="@+id/view13"
                            android:layout_width="18dp"
                            android:layout_height="wrap_content" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageButton
                                android:id="@+id/question"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:backgroundTint="#00FFFFFF"
                                android:padding="5dp"
                                android:scaleType="fitCenter"
                                app:srcCompat="@drawable/question" />

                            <TextView
                                android:id="@+id/question_name"
                                android:layout_width="wrap_content"
                                android:layout_height="18dp"
                                android:text="문의"
                                android:textAlignment="center"
                                android:textSize="12dp" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <!-- 하단 주요 버튼 및 이미지 -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="580dp"
                    android:background="@drawable/rectangle"
                    android:elevation="8dp"
                    android:orientation="vertical"
                    android:padding="20dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="21dp"
                            android:layout_marginBottom="18dp"
                            android:fontFamily="@font/inter_black"
                            android:text="당신이 찾으시는 "
                            android:textSize="20dp" />

                        <TextView
                            android:id="@+id/textView3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="18dp"
                            android:fontFamily="@font/inter_black"
                            android:text="정보"
                            android:textColor="#008791"
                            android:textSize="20dp" />

                        <TextView
                            android:id="@+id/textView4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="18dp"
                            android:fontFamily="@font/inter_black"
                            android:text="는?"
                            android:textSize="20dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageButton
                                android:id="@+id/tax_button"
                                android:layout_width="170dp"
                                android:layout_height="120dp"
                                android:layout_marginRight="4dp"
                                android:background="@null"
                                android:backgroundTint="#00FFFFFF"
                                android:padding="0dp"
                                android:scaleType="fitCenter"
                                app:srcCompat="@drawable/tax" />

                            <ImageButton
                                android:id="@+id/health_button"
                                android:layout_width="170dp"
                                android:layout_height="120dp"
                                android:background="@null"
                                android:backgroundTint="#00FFFFFF"
                                android:padding="0dp"
                                android:scaleType="fitCenter"
                                app:srcCompat="@drawable/health" />
                        </LinearLayout>

                        <View
                            android:id="@+id/view4"
                            android:layout_width="match_parent"
                            android:layout_height="5dp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageButton
                                android:id="@+id/tech_button"
                                android:layout_width="170dp"
                                android:layout_height="120dp"
                                android:layout_marginRight="4dp"
                                android:background="@null"
                                android:backgroundTint="#00FFFFFF"
                                android:padding="0dp"
                                android:scaleType="fitCenter"
                                app:srcCompat="@drawable/tech" />

                            <ImageButton
                                android:id="@+id/job_button"
                                android:layout_width="170dp"
                                android:layout_height="120dp"
                                android:background="@null"
                                android:backgroundTint="#00FFFFFF"
                                android:padding="0dp"
                                android:scaleType="fitCenter"
                                app:srcCompat="@drawable/job" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="5dp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageButton
                                android:id="@+id/personal_button"
                                android:layout_width="170dp"
                                android:layout_height="120dp"
                                android:layout_marginRight="4dp"
                                android:background="@null"
                                android:backgroundTint="#00FFFFFF"
                                android:padding="0dp"
                                android:scaleType="fitCenter"
                                app:srcCompat="@drawable/personal" />

                            <ImageButton
                                android:id="@+id/etc_button"
                                android:layout_width="170dp"
                                android:layout_height="120dp"
                                android:background="@null"
                                android:backgroundTint="#00FFFFFF"
                                android:padding="0dp"
                                android:scaleType="fitCenter"
                                app:srcCompat="@drawable/etc" />
                        </LinearLayout>

                        <ImageButton
                            android:id="@+id/imageButton9"
                            android:layout_width="350dp"
                            android:layout_height="90dp"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="8dp"
                            android:background="@null"
                            android:backgroundTint="#00FFFFFF"
                            android:padding="0dp"
                            android:scaleType="fitCenter"
                            app:srcCompat="@drawable/quesbutton" />
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </FrameLayout>

        <LinearLayout
            android:id="@+id/linearLayout6"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_gravity="bottom"
            android:layout_marginBottom="4dp"
            android:background="#ffffff"
            android:orientation="horizontal"
            android:elevation="10dp">

            <ImageButton
                android:id="@+id/imageButton11"
                android:layout_width="15dp"
                android:layout_height="wrap_content"
                android:layout_margin="0dp"
                android:layout_weight="1"
                android:background="@null"
                android:scaleType="fitCenter"
                app:srcCompat="@drawable/home_active" />

            <ImageButton
                android:id="@+id/imageButton5"
                android:layout_width="17dp"
                android:layout_height="65dp"
                android:layout_margin="0dp"
                android:layout_weight="1"
                android:background="@null"
                android:scaleType="fitCenter"
                app:srcCompat="@drawable/setting_1" />

        </LinearLayout>

        <FrameLayout
            android:id="@+id/menubar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#7C000000"
            android:clickable="true"
            android:elevation="10dp"
            android:focusable="true"
            android:visibility="invisible">

            <LinearLayout
                android:id="@+id/menu_panel"
                android:layout_width="191dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="220dp"
                android:background="#024C6F"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">


                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="15dp"
                        android:layout_marginTop="50dp"
                        android:layout_weight="1"
                        android:text="메뉴"
                        android:textColor="#ffffff"
                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <ImageButton
                        android:id="@+id/search_menu"
                        android:layout_width="15dp"
                        android:layout_height="25dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="50dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:scaleType="fitCenter"
                        app:srcCompat="@drawable/group_8__2_"
                        app:tint="#FFFFFF" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="40dp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="나의 정책"
                            android:textColor="#ffffff"
                            android:textSize="14dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/star_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:text="즐겨찾기"
                            android:textColor="#bbbbbb"
                            android:textSize="10dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="40dp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="청년 정책"
                            android:textColor="#ffffff"
                            android:textSize="14dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tax_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:text="세금 관련"
                            android:textColor="#bbbbbb"
                            android:textSize="10dp" />

                        <TextView
                            android:id="@+id/health_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:text="건강 관련"
                            android:textColor="#bbbbbb"
                            android:textSize="10dp" />

                        <TextView
                            android:id="@+id/monet_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:text="재테크 관련"
                            android:textColor="#bbbbbb"
                            android:textSize="10dp" />

                        <TextView
                            android:id="@+id/jop_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:text="취업 관련"
                            android:textColor="#bbbbbb"
                            android:textSize="10dp" />

                        <TextView
                            android:id="@+id/life_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:text="생활 관련"
                            android:textColor="#bbbbbb"
                            android:textSize="10dp" />

                        <TextView
                            android:id="@+id/school_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:text="학교 관련"
                            android:textColor="#bbbbbb"
                            android:textSize="10dp" />

                        <TextView
                            android:id="@+id/army_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:text="군대 관련"
                            android:textColor="#bbbbbb"
                            android:textSize="10dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="40dp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="주요 사이트"
                            android:textColor="#ffffff"
                            android:textSize="14dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/gov_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:text="정부 24"
                            android:textColor="#bbbbbb"
                            android:textSize="10dp" />

                        <TextView
                            android:id="@+id/work24_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:text="고용24"
                            android:textColor="#bbbbbb"
                            android:textSize="10dp" />

                        <TextView
                            android:id="@+id/bohum_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:text="건강보험공단"
                            android:textColor="#bbbbbb"
                            android:textSize="10dp" />

                        <TextView
                            android:id="@+id/hometax_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:text="홈택스"
                            android:textColor="#bbbbbb"
                            android:textSize="10dp" />

                        <TextView
                            android:id="@+id/janghak_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:text="한국장학재단"
                            android:textColor="#bbbbbb"
                            android:textSize="10dp" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

        </FrameLayout>

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>